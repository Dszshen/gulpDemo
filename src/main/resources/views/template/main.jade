doctype html
//if IE 8
    html.ie8.no-js(lang='en', data-ng-app='XXAPP')
//if IE 9
    html.ie9.no-js(lang='en', data-ng-app='XXAPP')
//if !IE | [if !IE]
html(lang='en', data-ng-app='XXAPP')
    // <![endif]
    // BEGIN HEAD
    head
        - var currTime = new Date().getTime();
        title(data-ng-bind="'XX管理系统 | ' + $state.current.data.pageTitle")
