        meta(http-equiv='X-UA-Compatible', content='IE=edge')
        meta(content='width=device-width, initial-scale=1', name='viewport')
        meta(content='', name='description')
        meta(content='', name='author')
        link(href='${request.contextPath}/assets/libs/fontawesome/css/font-awesome.css?t=#{currTime}', rel='stylesheet', type='text/css')
        link(href='${request.contextPath}/assets/libs/simple-line-icons/css/simple-line-icons.css?t=#{currTime}', rel='stylesheet', type='text/css')
        link(href='${request.contextPath}/assets/libs/bootstrap/dist/css/bootstrap.css?t=#{currTime}', rel='stylesheet', type='text/css')
        link(href='${request.contextPath}/assets/libs/jquery-uniform/css/uniform.default.css?t=#{currTime}', rel='stylesheet', type='text/css')
